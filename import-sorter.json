{
  "importStringConfiguration.tabSize": 2,
  "generalConfiguration.sortOnBeforeSave": true,
  "importStringConfiguration.hasSemicolon": false,
  "importSorter.importStringConfiguration.tabType": "space",
  "importSorter.importStringConfiguration.trailingComma": "none",
  "importStringConfiguration.numberOfEmptyLinesAfterAllImports": 1,
  "importStringConfiguration.maximumNumberOfImportExpressionsPerLine.count": 80,
  "importStringConfiguration.maximumNumberOfImportExpressionsPerLine.type": "newLineEachExpressionAfterCountLimitExceptIfOnlyOne",
  "sortConfiguration": {
    "customOrderingRules": {
      "rules": [
        {
          "type": "path",
          "regex": "^$",
          "orderLevel": 0,
          "disableSort": true
        },
        {
          "regex": "^react$|^./styles$|^react-native$",
          "orderLevel": 1,
          "disableSort": true
        },
        {
          "regex": "^./|^../",
          "orderLevel": 2,
          "disableSort": true
        },
        {
          "regex": "^(expo|@expo)-",
          "orderLevel": 3,
          "disableSort": true
        },
        {
          "regex": "^styled-components/native$",
          "orderLevel": 4,
          "disableSort": true
        },
        {
          "regex": "^(src/pages/)|(src/App)",
          "orderLevel": 5,
          "disableSort": true
        },
        {
          "regex": "^src/components/",
          "orderLevel": 6,
          "disableSort": true
        },
        {
          "regex": "^src/styles/",
          "orderLevel": 7,
          "disableSort": true
        },
        {
          "regex": "^src/services/",
          "orderLevel": 8,
          "disableSort": true
        },
        {
          "regex": "^src/store/",
          "orderLevel": 9,
          "disableSort": true
        },
        {
          "regex": "^src/utils/",
          "orderLevel": 10,
          "disableSort": true
        },
        {
          "regex": "^static/",
          "orderLevel": 11,
          "disableSort": true
        }
      ]
    }
  }
}
